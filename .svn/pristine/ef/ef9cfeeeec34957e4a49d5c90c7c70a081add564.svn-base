                    {
                        IWebElement details = GenericUtils.WaitForElementClickable(driver, By.XPath("//div[@class='wallet-card-grid']/div[" + i + "]/div[3]/div/a"), 10);
                        GenericUtils.ActionClick(driver, details);
                        break;
                    }
                }
            }
            catch (Exception e)
            {
                throw e;
            }

        }


